{
    "name": "surabayacoder/laravel-sage",
    "description": "A Laravel package for Retrieval-Augmented Generation (RAG) with Google Gemini.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Heru Afandi",
            "email": "heruaffandi06@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework": "^12.0",
        "spatie/pdf-to-text": "^1.52",
        "google-gemini-php/client": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "phpunit/phpunit": "^11.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Surabayacoder\\Sage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Surabayacoder\\Sage\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Surabayacoder\\Sage\\SageServiceProvider"
            ],
            "aliases": {
                "Sage": "Surabayacoder\\Sage\\Facades\\Sage"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
